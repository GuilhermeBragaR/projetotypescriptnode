    "typescript",
    "api"
  ],
  "author": "Guilherme Braga",
  "license": "ISC",
  "dependencies": {
    "module-alias": "^2.2.3",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/node": "^20.4.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "ts-node-dev": "^2.0.0"
  }
}
